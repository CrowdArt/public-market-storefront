sidekiq: bundle exec sidekiq -C config/sidekiq.yml
sidekiq-uploads: bundle exec rails runner "p q = Spree::Vendor.pluck(:slug).map {|s| \"-q #{s}-uploads, 1\"}; system(\"sidekiq -c 25 -C '' #{q.join(' ')}\")"
web: bundle exec rails server -b 0.0.0.0 -p 5000
es: elasticsearch
mailhog: bin/mailhog
reputation: bin/global_reputation
